<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    app:cardCornerRadius="@dimen/_3sdp"
    >

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/textView14"
            style="@style/TextInter700"
            android:layout_width="@dimen/_200sdp"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_21sdp"
            android:layout_marginTop="@dimen/_16sdp"
            android:layout_marginEnd="@dimen/_21sdp"
            android:text="@string/str_remove_photo"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


        <TextView
            android:id="@+id/textView15"
            android:layout_width="@dimen/_200sdp"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:text="@string/str_add_foto_profil"
            app:layout_constraintEnd_toEndOf="@+id/textView14"
            app:layout_constraintStart_toStartOf="@+id/textView14"
            app:layout_constraintTop_toBottomOf="@+id/textView14" />

        <Button
            android:id="@+id/dialogBtnCancelImgProfile"
            style="@style/OutlineButtonBlue"
            android:layout_width="@dimen/_76sdp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_10sdp"
            android:text="@string/str_cancel"
            android:textColor="@color/darker"
            app:layout_constraintBottom_toBottomOf="@+id/dialogBtnRemove"
            app:layout_constraintEnd_toStartOf="@+id/dialogBtnRemove"
            app:layout_constraintTop_toTopOf="@+id/dialogBtnRemove" />

        <Button
            android:id="@+id/dialogBtnRemove"
            style="@style/DefaultButtonBlue"
            android:layout_width="@dimen/_76sdp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_19sdp"
            android:layout_marginEnd="@dimen/_21sdp"
            android:layout_marginBottom="@dimen/_15sdp"
            android:text="@string/str_remove"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView15" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.cardview.widget.CardView>